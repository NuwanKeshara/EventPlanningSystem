<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Online_Event_Planning_System</display-name>
	<servlet>
		<servlet-name>mapLogin</servlet-name>
		<servlet-class>com.oeps.servlet.LoginValidate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>mapLogin</servlet-name>
		<url-pattern>/Home</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>com.oeps.servlet.AddUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ViewClients</servlet-name>
		<servlet-class>com.oeps.servlet.ViewClients</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewClients</servlet-name>
		<url-pattern>/clients</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ViewAdmins</servlet-name>
		<servlet-class>com.oeps.servlet.ViewAdmins</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewAdmins</servlet-name>
		<url-pattern>/admins</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>TempHome.jsp</welcome-file>
	</welcome-file-list>
</web-app>